<?xml version="1.0" encoding="UTF-8"?>
<root>
    <record model="webclient.view" xmlid="webclient.view_1">
        <field name="model_name" t="str">core.xmlid</field>
        <field name="xml" t="str">
            <view component="ListView">
                <properties>
                </properties>
                <definition>
                    <head>
                        <headCell>ID</headCell>
                        <headCell>xmlid</headCell>
                        <headCell>ID (red)</headCell>
                    </head>
                    <row>
                        <cell><field name="id" style="color: green;"/></cell>
                        <cell><field name="xmlid"/></cell>
                        <cell>
                            <div style="color: red;">
                                <field name="id"/>
                            </div>
                        </cell>
                    </row>
                </definition>
            </view>
        </field>
    </record>

    <record model="webclient.view" xmlid="webclient.view_2">
        <field name="model_name" t="str">core.xmlid</field>
        <field name="xml" t="str">
            <view component="FormView">
                <properties>
                </properties>
                <definition>
                    <head>
                        <h1>Hewwo!</h1>
                    </head>
                    <form>
                        <field name="id" style="color: green;"/>
                        <field name="xmlid" label="XML ID" editable="1"/>
                        <div style="color: red;">
                            <field name="id"/>
                        </div>
                    </form>
                </definition>
            </view>
        </field>
    </record>

    <record model="webclient.action" xmlid="webclient.xmlid_action">
        <field name="view_id" t="xmlid_ref">webclient.view_1</field>
        <field name="domain" t="str">[["xmlid", "ilike", "webclient."]]</field>
    </record>

    <record model="webclient.action" xmlid="webclient.xmlid_action2">
        <field name="view_id" t="xmlid_ref">webclient.view_1</field>
        <field name="domain" t="str">[]</field>
    </record>

    <record model="webclient.menuitem" xmlid="webclient.menuitem1">
        <field name="name" t="str">xmlid test thingy :3</field>
        <field name="action_id" t="xmlid_ref">webclient.xmlid_action</field>
    </record>

    <record model="webclient.menuitem" xmlid="webclient.menuitem2">
        <field name="name" t="str">submenu 1 L1</field>
        <field name="parent_id" t="xmlid_ref">webclient.menuitem1</field>
    </record>

    <record model="webclient.menuitem" xmlid="webclient.menuitem3">
        <field name="name" t="str">submenu 2 L1</field>
        <field name="parent_id" t="xmlid_ref">webclient.menuitem1</field>
    </record>

    <record model="webclient.menuitem" xmlid="webclient.menuitem4">
        <field name="name" t="str">submenu 3 L1</field>
        <field name="parent_id" t="xmlid_ref">webclient.menuitem1</field>
    </record>

    <record model="webclient.menuitem" xmlid="webclient.menuitem5">
