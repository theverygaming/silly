    <record model="webclient.menuitem" xmlid="webclient.menuitem7">
        <field name="name" t="str">submenu 3 L2 3 - ALL xmlids</field>
        <field name="parent_id" t="xmlid_ref">webclient.menuitem4</field>
        <field name="action_id" t="xmlid_ref">webclient.xmlid_action2</field>
    </record>
</root>
